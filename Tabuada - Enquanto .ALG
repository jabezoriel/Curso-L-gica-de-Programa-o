algoritmo "semnome"
var
   cont,tab,soma:inteiro
inicio
      cont <- 1
      Escreva ("Qual tabuada voce quer ver? :")
      leia (tab)
      Enquanto (cont <= 10) faca
      soma <- cont * tab
      EscrevaL (tab," x ", cont, "=",soma)
      cont <- cont + 1
      Fimenquanto
      
fimalgoritmo