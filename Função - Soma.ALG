algoritmo "semnome"
var
   n1,n2,res:inteiro
Funcao soma(a,b:inteiro):inteiro
var
   S:inteiro
inicio
   S <- A+B
   Retorne S
Fimfuncao
inicio
      Escreva ("Digite um número:")
      leia (n1)
      Escreva ("Digite outro número:")
      Leia (n2)

      Res <- soma(n1,n2)
      Escreva (" A soma é:", res)
fimalgoritmo